entrypoint = "gateway-server.js"
modules = ["nodejs-20"]

[workflows]
runButton = "Vecto Pilot"

[[workflows.workflow]]
name = "Vecto Pilot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"

[nix]
packages = ["nodejs-22", "lsof"]
channel = "stable-25_05"

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
build = "npm run build"
run = ["sh", "-c", "npm start"]

[env]
NODE_ENV = "development"

