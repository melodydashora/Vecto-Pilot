{
  "name": "eidolon-enhanced-repl",
  "version": "4.1.0",
  "description": "Eidolon - Claude Opus 4.1 Enhanced SDK (Environment-Agnostic)",
  "main": "gateway-server.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node gateway-server.js",
    "dev": "NODE_ENV=development node gateway-server.js",
    "db:push": "drizzle-kit generate:pg && drizzle-kit up:pg",
    "build": "vite build",
    "eidolon-direct": "node index.js",
    "gateway:proxy": "tsx server/gateway/assistant-proxy.ts",
    "agent:server": "tsx server/agent/index.ts",
    "override:start": "npm-run-all --parallel gateway:proxy agent:server",
    "build:client": "vite build",
    "dev:client": "vite dev",
    "lint": "eslint client/src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "emergency": "node emergency-eidolon.js",
    "recover": "bash eidolon-recovery.sh",
    "agent": "node agent-server.js",
    "test:anthropic": "node tests/eidolon/test-anthropic-model.js",
    "test:sdk": "node tests/eidolon/test-sdk-minimal.js",
    "test:protocol": "node tests/eidolon/test-exact-protocol.js",
    "test:app": "open tests/app/test-smart-blocks-visual.html",
    "test:override": "node tests/eidolon/test-override-confirmation.js",
    "test:phases": "node ./tests/run-all-phases.js",
    "debug:emergency": "node ./tools/debug/emergency-eidolon.js",
    "guard": "npm-run-all -s guard:json guard:deps guard:js guard:py",
    "guard:json": "jsonlint -q \"**/*.json\"",
    "guard:deps": "npm ls --all --omit=dev > /dev/null",
    "guard:js": "npm run lint",
    "guard:py": "command -v pyflakes >/dev/null 2>&1 && pyflakes . || true; command -v pycodestyle >/dev/null 2>&1 && pycodestyle . || true",
    "check:ts": "npm run typecheck || true",
    "build:dev": "npm run build:client",
    "build:deploy": "npm run guard && npm run build:client",
    "doctor": "echo '== Deps ==' && npm ls --omit=dev || true && echo '== Audit ==' && npm audit --omit=dev || true && echo '== Orphans ==' && depcheck || true",
    "model:research": "node tools/research/model-discovery.mjs",
    "model:verify": "node tools/research/model-discovery.mjs >/dev/null && echo 'âœ… Model verification passed'",
    "agent:build": "tsc -p tsconfig.agent.json",
    "agent:start": "node dist/index.js",
    "agent:dev": "ts-node src/index.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
