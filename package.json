    "eslint-plugin-react-refresh": "^0.4.6",
    "jsonlint": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.75.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
