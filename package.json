{
  "name": "eidolon-enhanced-repl",
  "version": "4.1.0",
  "description": "Eidolon - Claude Opus 4.1 Enhanced SDK (Environment-Agnostic)",
  "main": "gateway-server.js",
  "type": "module",
  "scripts": {
    "prestart:replit": "npm run build:client",
    "start:replit": "node scripts/start-replit.js",
    "build:client": "vite build",
    "start": "NODE_ENV=production node gateway-server.js",
    "dev": "NODE_ENV=development node gateway-server.js",
    "db:push": "drizzle-kit generate && drizzle-kit push",
    "db:migrate": "for f in migrations/*.sql; do echo \"Running $f...\"; psql $DATABASE_URL -f \"$f\"; done",
    "db:migrate:latest": "psql $DATABASE_URL -f migrations/20260114_create_places_cache.sql && psql $DATABASE_URL -f migrations/20260114_progressive_enrichment.sql",
    "build": "vite build",
    "dev:client": "vite dev",
    "lint": "eslint client/src --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "agent": "node agent-server.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest tests/blocksApi.test.js --verbose",
    "test:e2e": "playwright test tests/e2e/copilot.spec.ts",
    "test:seed": "node scripts/seed-dev.js && npm run test:unit",
    "guard": "npm-run-all -s guard:json guard:deps guard:js",
    "guard:json": "jsonlint -q \"**/*.json\"",
    "guard:deps": "npm ls --all --omit=dev > /dev/null",
    "guard:js": "npm run lint",
    "check:ts": "npm run typecheck || true",
    "build:dev": "npm run build:client",
    "build:deploy": "npm run guard && npm run build:client",
    "doctor": "echo '== Deps ==' && npm ls --omit=dev || true && echo '== Audit ==' && npm audit --omit=dev || true && echo '== Orphans ==' && depcheck || true",
    "start:mono": "./start-mono.sh",
    "research:models": "node tools/research/update-model-md.mjs"
  },
