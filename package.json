    "input-otp": "^1.4.2",
    "lucide-react": "^0.544.0",
    "md-to-pdf": "^5.2.4",
    "openai": "^5.23.2",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.63.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "uuid": "^13.0.0",
    "vaul": "^1.1.2",
    "vite": "^7.1.11",
    "wouter": "^3.7.1",
    "ws": "^8.18.3",
    "xml2js": "^0.6.2",
    "zod": "^3.25.76",
    "tailwindcss": "^3.4.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "assistant",
    "claude",
    "replit",
    "eidolon",
    "environment-agnostic"
  ],
  "author": "Eidolon Enhanced SDK",
  "license": "MIT",
  "devDependencies": {
    "@replit/vite-plugin-runtime-error-modal": "^0.0.3",
